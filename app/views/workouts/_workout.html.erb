<%= turbo_frame_tag workout do %> 
  <div id="<%= dom_id workout %>" class = "w-full">
      <div class="flex justify-between ">
            <%= link_to workout.name, workout_path(workout), data: { turbo_frame: "_top" }, class: "text-2xl font-bold antialiased hover:text-blue-600"%>

          <div class = "flex justify-between" >
              <%= button_to 'Destroy this workout', workout_path(workout), method: :delete, class: "rounded-lg py-4 ml-2 px-5 bg-gray-100 inline-block font-medium" %>

              <%= link_to 'Edit this workout', edit_workout_path(workout), class: "text-white bg-blue-700 hover:bg-blue-800 focus:ring-4 focus:ring-blue-300 font-medium rounded-lg text-lg px-5 py-2.5 mr-2 mb-2 dark:bg-blue-600 dark:hover:bg-blue-700 focus:outline-none dark:focus:ring-blue-800" %></br></br></br>
          </div>
        </div>
    </div>
<% end %>

